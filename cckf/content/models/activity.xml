<?xml version="1.0" ?>
<model i18n:domain="cckf.content"
 xmlns="http://namespaces.plone.org/supermodel/schema"
 xmlns:form="http://namespaces.plone.org/supermodel/form"
 xmlns:i18n="http://xml.zope.org/namespaces/i18n">

<schema>

  <field name="id"
   type="zope.schema.TextLine">
    <title i18n:translate="">Id</title>
    <required>False</required>
  </field>
  <field name="title"
   type="zope.schema.TextLine">
    <title i18n:translate="">Title</title>
  </field>
  <field name="description"
   type="zope.schema.Text">
    <title i18n:translate="">Description</title>
    <required>False</required>
  </field>
  <field name="remoteUrl"
   type="zope.schema.Tuple">
    <title i18n:translate="">URL</title>
    <description i18n:translate="">Link to the Resource.</description>
    <value_type type="zope.schema.TextLine">
    <title>URL</title>
    </value_type>
    <required>False</required>
  </field>
  <field name="body"
   type="plone.app.textfield.RichText">
    <title i18n:translate="">Text</title>
    <description />
    <required>False</required>
  </field>

</schema>

</model>
