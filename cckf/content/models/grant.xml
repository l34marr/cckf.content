<?xml version="1.0" ?>
<model i18n:domain="cckf.content"
 xmlns="http://namespaces.plone.org/supermodel/schema"
 xmlns:form="http://namespaces.plone.org/supermodel/form"
 xmlns:i18n="http://xml.zope.org/namespaces/i18n">

<schema>

  <field name="id"
   type="zope.schema.TextLine">
    <title i18n:translate="">Id</title>
  </field>
  <field name="title"
   type="zope.schema.TextLine">
    <title i18n:translate="">Project</title>
  </field>
  <field name="year"
   type="zope.schema.TextLine">
    <title i18n:translate="">Year</title>
    <required>False</required>
  </field>
  <field name="director"
   type="zope.schema.TextLine">
    <title i18n:translate="">Director</title>
    <required>False</required>
  </field>
  <field name="description"
   type="zope.schema.Text">
    <title i18n:translate="g_institute">Institute</title>
    <required>False</required>
  </field>
  <field name="department"
   type="zope.schema.TextLine">
    <title i18n:translate="">Department</title>
    <required>False</required>
  </field>
  <field name="region"
   type="zope.schema.Choice">
    <title i18n:translate="">Region</title>
    <required>False</required>
    <vocabulary>cckf.region</vocabulary>
  </field>
  <field name="category"
   type="zope.schema.Choice"
   form:widget="z3c.form.browser.radio.RadioFieldWidget">
    <title i18n:translate="">Category</title>
    <required>False</required>
    <vocabulary>cckf.category</vocabulary>
  </field>
  <field name="currency"
   type="zope.schema.TextLine">
    <title i18n:translate="">Currency</title>
    <required>False</required>
  </field>
  <field name="budget"
   type="zope.schema.TextLine">
    <title i18n:translate="">Budget</title>
    <required>False</required>
  </field>
  <field name="status"
   type="zope.schema.TextLine">
    <title i18n:translate="">Status</title>
    <required>False</required>
  </field>
  <field name="file"
   type="plone.namedfile.field.NamedBlobFile">
    <description />
    <title i18n:translate="">File</title>
    <required>False</required>
  </field>

</schema>

</model>
